import { TruffleContract } from 'truffle-contract';
import { TruffleArtifacts } from 'truffle';
import { 
    {{#each contracts}}
    {{contractInstanceName}}{{#unless @last}},{{/unless}}
    {{/each}}
} from '{{generatedDir}}'

export interface TruffleArtifacts {
    {{#each contracts}}
	require(name: '{{contractArtifactName}}'): TruffleContract<{{contractInstanceName}}>;
    {{/each}}
}